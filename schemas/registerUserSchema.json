{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "firstName",
        "lastName",
        "username",
        "password"
    ],
    "properties": {
        "firstName": {
            "type": "string",
            "default": "",
            "title": "The firstName Schema",
            "examples": [
                "John"
            ]
        },
        "lastName": {
            "type": "string",
            "default": "",
            "title": "The lastName Schema",
            "examples": [
                "Doe"
            ]
        },
        "username": {
            "type": "string",
            "default": "",
            "title": "The username Schema",
            "examples": [
                "blue"
            ]
        },
        "password": {
            "type": "string",
            "default": "",
            "title": "The password Schema",
            "examples": [
                "test"
            ]
        }
    },
    "examples": [{
        "first_name": "John",
        "last_name": "Doe",
        "username": "blue",
        "password": "test"
    }]
}