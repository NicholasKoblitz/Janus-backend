{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "courseId",
        "username"
    ],
    "properties": {
        "courseId": {
            "title": "The courseId Schema",
            "type": "string",
            "default": "",
            "examples": [
                "MAT101"
            ]
        },
        "username": {
            "title": "The username Schema",
            "type": "string",
            "default": 0,
            "examples": [
                "blue"
            ]
        }
    },
    "examples": [{
        "course_id": "MAT101",
        "user_id": 1
    }]
}